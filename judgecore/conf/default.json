{
  "debug": false,
  "pid": null,
  "sid": null,
  "filename": null,
  "lang": "c",
  "variant": {
    "compiler": "gcc",
    "cargs": ["-DONLINE_JUDGE", "-lpthread", "-O2", "-static", "-std=c11", "-fno-asm", "-Wall", "-Wextra", "-o", "$exec", "$code"],
    "cscript": [],
    "executable": "$exec",
    "eargs": []
  },
  "max_time": 1000,
  "max_real_time": 2000,
  "max_time_total": 30000,
  "max_memory": 65530,
  "max_output": 10000000,
  "max_thread": 4,
  "continue_on": ["accepted", "presentation error"],
  "test_case_count": 1,
  "spj_mode": "no",
  "path": {
    "base": "/mnt/data",
    "code": ["$base", "code", "$pid", "$sid", "$filename"],
    "log": ["$temp"],
    "output": ["$base", "result", "$pid", "$sid"],
    "stdin": ["$base", "case", "$pid"],
    "stdout": ["$stdin"],
    "temp": ["/tmp", ["judge-", "$pid", "-", "$sid"]],
    "exec": ["$temp", "main"],
    "spj": ["$base", "judge", "$pid"]
  },
  "inline": [{
    "stdin": "1 2\n",
    "fs": {
      "hello.txt": "Hello world!\n"
    }
  }]
}
